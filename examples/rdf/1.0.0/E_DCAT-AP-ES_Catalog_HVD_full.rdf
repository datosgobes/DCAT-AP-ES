<!--
################################################################################
# DCAT-AP-ES HVD Full Conformance Example (versión 1.0.0)
# Incluye todas las propiedades obligatorias (Ob), recomendadas (R) y HVD
# Incluye propiedades opcionales (O) representativas para demostrar conformidad HVD completa
# Referencia: https://datosgobes.github.io/DCAT-AP-ES/
# HVD Regulation: EU Implementing Regulation 2023/138
################################################################################
-->
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:dcat="http://www.w3.org/ns/dcat#"
         xmlns:dct="http://purl.org/dc/terms/"
         xmlns:foaf="http://xmlns.com/foaf/0.1/"
         xmlns:dcatap="http://data.europa.eu/r5r/"
         xmlns:vcard="http://www.w3.org/2006/vcard/ns#"
         xmlns:adms="http://www.w3.org/ns/adms#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#">

  <!--
  ################################################################################
  # 1. Catálogo HVD: dcat:Catalog (todas las propiedades obligatorias, recomendadas y HVD)
  ################################################################################
  -->
  <dcat:Catalog rdf:about="http://dcat-ap-es.ejemplo.org/catalogo-hvd-completo">
    <!-- Obligatorias (Ob) -->
    <dct:title xml:lang="es">Catálogo de Datos de Alto Valor - España</dct:title>
    <dct:title xml:lang="en">High Value Datasets Catalog - Spain</dct:title>
    <dct:description xml:lang="es">Catálogo nacional de datos de alto valor (HVD) conforme al Reglamento de Ejecución (UE) 2023/138 de la Comisión. Contiene conjuntos de datos en categorías temáticas prioritarias para reutilización.</dct:description>
    <dct:description xml:lang="en">National high value datasets (HVD) catalog compliant with Commission Implementing Regulation (EU) 2023/138. Contains datasets in priority thematic categories for re-use.</dct:description>
    <dct:publisher rdf:resource="http://datos.gob.es/recurso/sector-publico/org/Organismo/E0DAT0001"/>
    <foaf:homepage rdf:resource="http://dcat-ap-es.ejemplo.org/hvd"/>
    <dcat:themeTaxonomy rdf:resource="http://datos.gob.es/kos/sector-publico/sector"/>
    <dct:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2024-01-01T00:00:00+01:00</dct:issued>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2025-01-15T10:30:00+01:00</dct:modified>
    <dct:language rdf:resource="http://publications.europa.eu/resource/authority/language/SPA"/>
    <dct:language rdf:resource="http://publications.europa.eu/resource/authority/language/ENG"/>
    <dct:license rdf:resource="http://publications.europa.eu/resource/authority/licence/CC0"/>

    <!-- Recomendadas (R) -->
    <dct:identifier>http://dcat-ap-es.ejemplo.org/catalogo-hvd-completo</dct:identifier>
    <dct:spatial rdf:resource="http://datos.gob.es/recurso/sector-publico/territorio/Pais/España"/>
    
    <!-- Opcionales (O) representativas -->
    <dct:rights rdf:resource="http://publications.europa.eu/resource/authority/access-right/PUBLIC"/>
    <dcat:contactPoint>
      <vcard:Organization rdf:about="http://dcat-ap-es.ejemplo.org/contacto-catalogo-hvd">
        <vcard:organization-name xml:lang="es">datos.gob.es</vcard:organization-name>
        <vcard:fn xml:lang="es">Oficina de Datos de Alto Valor</vcard:fn>
        <vcard:fn xml:lang="en">High Value Data Office</vcard:fn>
        <vcard:hasEmail rdf:resource="mailto:hvd-datos@ejemplo.gob.es"/>
        <vcard:hasTelephone rdf:resource="tel:+34-900999999"/>
        <vcard:hasURL rdf:resource="http://dcat-ap-es.ejemplo.org/hvd/contacto"/>
        <vcard:hasUID rdf:resource="http://datos.gob.es/recurso/sector-publico/org/Organismo/E0DAT0001"/>
      </vcard:Organization>
    </dcat:contactPoint>

    <!--
    ################################################################################
    # 2. DataService HVD: dcat:DataService (todas las propiedades obligatorias, recomendadas y HVD)
    ################################################################################
    -->
    <dcat:service>
      <dcat:DataService rdf:about="http://dcat-ap-es.ejemplo.org/dataservice/api-geoespacial-hvd">
        <!-- Obligatorias (Ob) para DataService -->
        <dct:title xml:lang="es">API Geoespacial de Datos de Alto Valor</dct:title>
        <dct:title xml:lang="en">HVD Geospatial Data API</dct:title>
        <dcat:endpointURL rdf:resource="https://api.dcat-ap-es.ejemplo.org/geoespacial/v1"/>
        <dcat:theme rdf:resource="http://datos.gob.es/kos/sector-publico/sector/medio-ambiente"/>
        <dct:publisher rdf:resource="http://datos.gob.es/recurso/sector-publico/org/Organismo/E0DAT0001"/>
        
        <!-- HVD Obligatorias -->
        <dcatap:hvdCategory rdf:resource="http://data.europa.eu/bna/c_164e0bf5"/>
        <dcatap:applicableLegislation rdf:resource="http://data.europa.eu/eli/reg_impl/2023/138/oj"/>
        
        <!-- Recomendadas (R) -->
        <dct:description xml:lang="es">Servicio API RESTful para acceso a datos geoespaciales de alto valor. Proporciona endpoints OGC-compliant para WFS, WMS y CSW.</dct:description>
        <dct:description xml:lang="en">RESTful API service for high value geospatial data access. Provides OGC-compliant endpoints for WFS, WMS and CSW.</dct:description>
        <dcat:endpointDescription rdf:resource="https://api.dcat-ap-es.ejemplo.org/geoespacial/v1/swagger.json"/>
        
        <!-- Opcionales (O) representativas -->
        <dct:identifier>http://dcat-ap-es.ejemplo.org/dataservice/api-geoespacial-hvd</dct:identifier>
        <dct:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2024-06-09T00:00:00+01:00</dct:issued>
        <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2025-01-10T14:00:00+01:00</dct:modified>
        <dcat:keyword xml:lang="es">geoespacial</dcat:keyword>
        <dcat:keyword xml:lang="es">API</dcat:keyword>
        <dcat:keyword xml:lang="es">OGC</dcat:keyword>
        <dcat:keyword xml:lang="en">geospatial</dcat:keyword>
        <dcat:keyword xml:lang="en">API</dcat:keyword>
        <dcat:keyword xml:lang="en">OGC</dcat:keyword>
        <dct:license rdf:resource="http://publications.europa.eu/resource/authority/licence/CC0"/>
        <dct:accessRights rdf:resource="http://publications.europa.eu/resource/authority/access-right/PUBLIC"/>
        <dct:conformsTo rdf:resource="http://www.opengis.net/def/serviceType/ogc/wfs"/>
        <dct:conformsTo rdf:resource="http://www.opengis.net/def/serviceType/ogc/wms"/>
        <foaf:page rdf:resource="https://dcat-ap-es.ejemplo.org/hvd/api/sla.html"/>
        <dcat:contactPoint>
          <vcard:Organization rdf:about="http://dcat-ap-es.ejemplo.org/contacto-dataservice-hvd">
            <vcard:organization-name xml:lang="es">Oficina Técnica de Servicios Geoespaciales</vcard:organization-name>
            <vcard:fn xml:lang="es">Soporte API Geoespacial</vcard:fn>
            <vcard:fn xml:lang="en">Geospatial API Support</vcard:fn>
            <vcard:hasEmail rdf:resource="mailto:api-geo@ejemplo.gob.es"/>
            <vcard:hasTelephone rdf:resource="tel:+34-900999999"/>
            <vcard:hasURL rdf:resource="https://api.dcat-ap-es.ejemplo.org/support"/>
            <vcard:hasUID rdf:resource="http://datos.gob.es/recurso/sector-publico/org/Organismo/E0DAT0001"/>
          </vcard:Organization>
        </dcat:contactPoint>
        <dcat:servesDataset rdf:resource="http://dcat-ap-es.ejemplo.org/dataset/red-transporte-hvd"/>
      </dcat:DataService>
    </dcat:service>

    <!--
    ################################################################################
    # 3. Dataset HVD: dcat:Dataset (todas las propiedades obligatorias, recomendadas y HVD)
    ################################################################################
    -->
    <dcat:dataset>
      <dcat:Dataset rdf:about="http://dcat-ap-es.ejemplo.org/dataset/red-transporte-hvd">
        <!-- Obligatorias (Ob) -->
        <dct:title xml:lang="es">Red de Transporte Público Nacional - Datos de Alto Valor</dct:title>
        <dct:title xml:lang="en">National Public Transport Network - High Value Data</dct:title>
        <dct:description xml:lang="es">Conjunto de datos de alto valor sobre la red de transporte público español, incluyendo horarios, rutas, paradas, accesibilidad y datos en tiempo real. Conforme al Reglamento (UE) 2023/138.</dct:description>
        <dct:description xml:lang="en">High value dataset about the Spanish public transport network, including schedules, routes, stops, accessibility and real-time data. Compliant with Regulation (EU) 2023/138.</dct:description>
        <dct:publisher rdf:resource="http://datos.gob.es/recurso/sector-publico/org/Organismo/E0DAT0001"/>
        <dcat:theme rdf:resource="http://datos.gob.es/kos/sector-publico/sector/transporte"/>

        <!-- HVD Obligatorias -->
        <dcatap:hvdCategory rdf:resource="http://data.europa.eu/bna/c_ac64a52d"/>
        <dcatap:applicableLegislation rdf:resource="http://data.europa.eu/eli/reg_impl/2023/138/oj"/>

        <!-- Recomendadas (R) -->
        <dct:identifier>http://dcat-ap-es.ejemplo.org/dataset/red-transporte-hvd</dct:identifier>
        <dct:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2024-06-09T00:00:00+01:00</dct:issued>
        <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2025-01-15T08:00:00+01:00</dct:modified>
        <dcat:keyword xml:lang="es">transporte público</dcat:keyword>
        <dcat:keyword xml:lang="es">movilidad</dcat:keyword>
        <dcat:keyword xml:lang="es">alto valor</dcat:keyword>
        <dcat:keyword xml:lang="es">GTFS</dcat:keyword>
        <dcat:keyword xml:lang="en">public transport</dcat:keyword>
        <dcat:keyword xml:lang="en">mobility</dcat:keyword>
        <dcat:keyword xml:lang="en">high value</dcat:keyword>
        <dcat:keyword xml:lang="en">GTFS</dcat:keyword>
        <dct:spatial rdf:resource="http://datos.gob.es/recurso/sector-publico/territorio/Pais/España"/>

        <!-- Opcionales (O) representativas -->
        <dct:accrualPeriodicity rdf:resource="http://publications.europa.eu/resource/authority/frequency/DAILY"/>
        <dct:temporal>
          <dct:PeriodOfTime>
            <dcat:startDate rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2024-06-09T00:00:00+01:00</dcat:startDate>
          </dct:PeriodOfTime>
        </dct:temporal>
        <dcat:landingPage rdf:resource="http://dcat-ap-es.ejemplo.org/dataset/red-transporte-hvd"/>
        <dct:conformsTo rdf:resource="https://gtfs.org/schedule/reference/"/>
        <dct:conformsTo rdf:resource="https://gtfs.org/realtime/reference/"/>
        <dct:accessRights rdf:resource="http://publications.europa.eu/resource/authority/access-right/PUBLIC"/>
        <adms:identifier>
          <adms:Identifier>
            <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ES-HVD-TRANSPORT-2024-001</skos:notation>
          </adms:Identifier>
        </adms:identifier>
        <dcat:contactPoint>
          <vcard:Organization rdf:about="http://dcat-ap-es.ejemplo.org/contacto-dataset-transporte">
            <vcard:organization-name xml:lang="es">datos.gob.es</vcard:organization-name>
            <vcard:fn xml:lang="es">Oficina de Datos de Transporte</vcard:fn>
            <vcard:fn xml:lang="en">Transport Data Office</vcard:fn>
            <vcard:hasEmail rdf:resource="mailto:datos-transporte@ejemplo.gob.es"/>
            <vcard:hasTelephone rdf:resource="tel:+34-915556789"/>
            <vcard:hasURL rdf:resource="http://dcat-ap-es.ejemplo.org/transporte/contacto"/>
            <vcard:hasUID rdf:resource="http://datos.gob.es/recurso/sector-publico/org/Organismo/E0DAT0001"/>
          </vcard:Organization>
        </dcat:contactPoint>

        <!--
        ################################################################################
        # 4. Distribution HVD: dcat:Distribution (todas las propiedades obligatorias, recomendadas y HVD)
        ################################################################################
        -->
        <dcat:distribution>
          <dcat:Distribution rdf:about="http://dcat-ap-es.ejemplo.org/distribucion/red-transporte-gtfs">
            <!-- Obligatorias (Ob) -->
            <dcat:accessURL rdf:resource="https://dcat-ap-es.ejemplo.org/download/transporte-gtfs.zip"/>

            <!-- Recomendadas (R) -->
            <dct:title xml:lang="es">Distribución GTFS - Red Transporte Nacional</dct:title>
            <dct:title xml:lang="en">GTFS Distribution - National Transport Network</dct:title>
            <dct:description xml:lang="es">Archivo GTFS completo con horarios, rutas y paradas del transporte público nacional.</dct:description>
            <dct:description xml:lang="en">Complete GTFS file with schedules, routes and stops of the national public transport.</dct:description>
            <dct:format rdf:resource="http://publications.europa.eu/resource/authority/file-type/ZIP"/>
            <dct:license rdf:resource="http://publications.europa.eu/resource/authority/licence/CC0"/>

            <!-- HVD Recomendadas -->
            <dcatap:availability rdf:resource="http://publications.europa.eu/resource/authority/planned-availability/STABLE"/>

            <!-- Opcionales (O) representativas -->
            <dct:identifier>http://dcat-ap-es.ejemplo.org/distribucion/red-transporte-gtfs</dct:identifier>
            <dct:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2024-06-09T00:00:00+01:00</dct:issued>
            <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2025-01-15T06:00:00+01:00</dct:modified>
            <dcat:downloadURL rdf:resource="https://dcat-ap-es.ejemplo.org/files/transporte-gtfs.zip"/>
            <dcat:byteSize rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">45678901</dcat:byteSize>
            <dcat:mediaType>application/zip</dcat:mediaType>
            <adms:status rdf:resource="http://purl.org/adms/status/Completed"/>
            <dct:rights rdf:resource="http://publications.europa.eu/resource/authority/access-right/PUBLIC"/>
            <dct:conformsTo rdf:resource="https://gtfs.org/schedule/reference/"/>
            <dcat:compressFormat rdf:resource="http://www.iana.org/assignments/media-types/application/zip"/>
            <dcat:packageFormat rdf:resource="http://publications.europa.eu/resource/authority/file-type/ZIP"/>
          </dcat:Distribution>
        </dcat:distribution>

        <!-- Distribución adicional: API en tiempo real -->
        <dcat:distribution>
          <dcat:Distribution rdf:about="http://dcat-ap-es.ejemplo.org/distribucion/red-transporte-gtfs-rt">
            <dcat:accessURL rdf:resource="https://api.dcat-ap-es.ejemplo.org/transporte/gtfs-realtime"/>
            <dct:title xml:lang="es">Distribución GTFS Realtime - Datos en Tiempo Real</dct:title>
            <dct:title xml:lang="en">GTFS Realtime Distribution - Real-Time Data</dct:title>
            <dct:description xml:lang="es">API de datos en tiempo real del transporte público en formato GTFS Realtime (Protocol Buffers).</dct:description>
            <dct:description xml:lang="en">Real-time public transport data API in GTFS Realtime format (Protocol Buffers).</dct:description>
            <dct:format rdf:resource="http://publications.europa.eu/resource/authority/file-type/PROTOCOL_BUF"/>
            <dct:license rdf:resource="http://publications.europa.eu/resource/authority/licence/CC0"/>
            <dcatap:availability rdf:resource="http://publications.europa.eu/resource/authority/planned-availability/AVAILABLE"/>
            <dct:identifier>http://dcat-ap-es.ejemplo.org/distribucion/red-transporte-gtfs-rt</dct:identifier>
            <dct:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2024-06-09T00:00:00+01:00</dct:issued>
            <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2025-01-15T08:00:00+01:00</dct:modified>
            <dcat:mediaType>application/x-protobuf</dcat:mediaType>
            <adms:status rdf:resource="http://purl.org/adms/status/UnderDevelopment"/>
            <dct:conformsTo rdf:resource="https://gtfs.org/realtime/reference/"/>
            <dcat:accessService rdf:resource="http://dcat-ap-es.ejemplo.org/dataservice/api-geoespacial-hvd"/>
          </dcat:Distribution>
        </dcat:distribution>

        <!-- Distribución adicional: CSV para análisis -->
        <dcat:distribution>
          <dcat:Distribution rdf:about="http://dcat-ap-es.ejemplo.org/distribucion/red-transporte-csv">
            <dcat:accessURL rdf:resource="https://dcat-ap-es.ejemplo.org/download/transporte-paradas.csv"/>
            <dct:title xml:lang="es">Distribución CSV - Paradas de Transporte</dct:title>
            <dct:title xml:lang="en">CSV Distribution - Transport Stops</dct:title>
            <dct:description xml:lang="es">Listado de todas las paradas de transporte público en formato CSV para análisis.</dct:description>
            <dct:description xml:lang="en">List of all public transport stops in CSV format for analysis.</dct:description>
            <dct:format rdf:resource="http://publications.europa.eu/resource/authority/file-type/CSV"/>
            <dct:license rdf:resource="http://publications.europa.eu/resource/authority/licence/CC0"/>
            <dcatap:availability rdf:resource="http://publications.europa.eu/resource/authority/planned-availability/STABLE"/>
            <dct:identifier>http://dcat-ap-es.ejemplo.org/distribucion/red-transporte-csv</dct:identifier>
            <dct:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2024-06-09T00:00:00+01:00</dct:issued>
            <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2025-01-15T06:00:00+01:00</dct:modified>
            <dcat:downloadURL rdf:resource="https://dcat-ap-es.ejemplo.org/files/transporte-paradas.csv"/>
            <dcat:byteSize rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">8901234</dcat:byteSize>
            <dcat:mediaType>text/csv</dcat:mediaType>
            <adms:status rdf:resource="http://purl.org/adms/status/Completed"/>
            <dct:rights rdf:resource="http://publications.europa.eu/resource/authority/access-right/PUBLIC"/>
          </dcat:Distribution>
        </dcat:distribution>
      </dcat:Dataset>
    </dcat:dataset>
  </dcat:Catalog>

  <!--
  ################################################################################
  # Notas sobre conformidad HVD completa
  ################################################################################
  # - Este ejemplo cumple con el Reglamento de Ejecución (UE) 2023/138
  # - Incluye todas las propiedades obligatorias HVD:
  #   * dcatap:hvdCategory en Dataset y DataService
  #   * dcatap:applicableLegislation en Dataset y DataService
  # - Incluye propiedades recomendadas HVD:
  #   * dcatap:availability en Distribution
  # - Licencia CC0 (Creative Commons Zero) para máxima reutilización
  # - API RESTful con endpoints documentados (Swagger/OpenAPI)
  # - Múltiples distribuciones: GTFS estático, GTFS Realtime, CSV
  # - Conformidad con estándares internacionales: GTFS, OGC WFS/WMS
  # - Metadatos de contacto completos con vcard:Organization
  # - Identificadores únicos para todas las entidades
  # - Fechas en formato xsd:dateTime con zona horaria
  # - Multilingüismo completo (español e inglés)
  # - SLA (Service Level Agreement) documentado
  # - Este ejemplo debe pasar la validación SHACL HVD sin warnings ni violations
  ################################################################################
  -->
</rdf:RDF>
